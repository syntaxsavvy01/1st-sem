#include <iostream>
using namespace std;

class number{
	int value;
	
	public:
		number(int v = 0){
			value = v;
		}
		
		friend int operator == (number n1,  number n2);
};		
		int operator == (number n1, number n2){
			return (n1.value == n2.value);
		}


int main(){
	number n1(10), n2(5);
	if(n1==n2){
		cout<<"Both are equal."<<endl;
	}
	else{
		cout<<"Both are not equal."<<endl;
	}
	return 0;
}
