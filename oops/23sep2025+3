//create array , initialize pointer and print element thorough pointer

#include<iostream>
using namespace std;

int main(){
	int a[5]={0,1,2,3,4};
	int *p[5];
	
	for(int i = 0; i<5 ; i++){
		 p[i] = &a[i];
	}
	
	for(int i = 0; i < 5; i++) {
        cout << *p[i] << endl;
    }
    
	return 0;
}

