#include<iostream>
using namespace std;

class distance{
	private:
		int feet, inches;
	
	public:
	distance()	{
		feet = 0;
		inches = 0;
	}
	
	distance( int f, int i){
		feet = f;
		inches = i;
	}
	
	void displaydistance(){
		cout<<"F: "<<feet<<"I: "<<inches<<endl;
	}
	
	//overloading minus(-) operator
	distance operator -(){
		feet=-feet;
		inches = -inches;
		return distance(feet,inches);
	}
};


int main(){
	distance d1(11,10), d2(-5,11);
	-d1;
	d1.displaydistance();
	-d2;
	d2.displaydistance();
	return 0;
}
