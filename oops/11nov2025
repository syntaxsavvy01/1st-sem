#include <iostream>
using namespace std;

int main() 
{
    int n;
    cout<<"Enter the number of elements: ";
    cin>>n;
    int *arr = new int [n];
    
    
    for(int i = 0 ; i < n ; i++){
      cout<<"Enter the values: ";
      cin>>arr[i];
    }
    
    
    for(int i = 0 ; i < n ; i++){
      cout<<arr[i]<<endl;

    }
    
    
    delete[] arr;
    return 0;
}
