select *from emp
	where joining_date > '20-03-2022';
-- Q1 find the number of male employee as total_male from "employee" table
select count(*) as total_male 
from emp
where gender='M';

-- Q2. find the number of employrr whosee first name starts with the letter 'J' from employee table.
select count(*) as total_j_employees
from emp
where first_name like 'j%';

-- Q3. find the total salary of employee whose employee numbers (emp_no) are between 1010 to 1020 from "employee" table.
select sum(salary) as total_salary
from emp
where emp_no between 1010 and 1020;

-- Q4. calculate the total annual salary of all employee as annual salary in the HR department from "employee" table.
select sum(salary) * 12 as annual_salary
from emp
where department = 'hr';

-- Q5. find the average salary of all employee as avg_salary from "employee" table.
select avg(salary) as avg_salary
from emp;

-- Q6. find the average salary of employee working in the IT department form employee table.
select avg(salary) as avg_salary
from emp
where department = 'it';

-- Q7. find the earliest joining date as min_joining_date among all employees in the employee table.
select min(joining_date) as min_joining_date
from emp;

-- Q8. find the minimum salary of employees working in the finance department as min_salary from "employee" table.
select min(salary) as min_salary
from emp
where department = 'finance';

-- Q9. find the maximum birth date among all employees from "employee" rtable as max_DOB.
select max(birth_date) as max_dob
from emp;

-- Q10. what is the higest employee number (emp_no) in the "employee" table.
select max(emp_no) as max_emp_no
from emp;

-- Q11. display employe name and employee ID whose salary is maximum.
select emp_no, first_name from emp
where salary=(select max(salary) from emp);

-- Q12. Find the number of female employees in the Marketing department.
select count(*) from emp where gender='f' and department ='marketing';

-- Q13. Retrieve the first name, last name, and department of employees whose salary is greater than the average salary.
select first_name, last_name, department from emp where salary>(select avg(salary) from emp);
