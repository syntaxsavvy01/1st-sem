


show databases;

create database employee;

use employee;

create table emp(
	emp_no int primary key,
    first_name varchar(10) not null,
    last_name varchar(10),
    salary int not null,
    birth_date date,
    gender varchar(6) ,
    joining_date date,
    department varchar(30));

